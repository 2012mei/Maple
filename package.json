{
  "name": "maple-bookmarks",
  "version": "1.7",
  "description": "Let you navigate smoothly while hiding the bookmark bar.",
  "main": "bookmark/popup.js",
  "scripts": {
    "bookmark": "zip -r ./resource/Maple.zip ./bookmark",
    "newtab": "zip -r ./resource/NewTab.zip ./newtab",
    "build": "npm run bookmark && npm run newtab"
  },
  "keywords": [
    "bookmarks",
    "bookmark",
    "chrome",
    "browser-extension"
  ],
  "author": "Tw93",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.0",
    "prettier": "^3.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
